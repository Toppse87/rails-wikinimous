<%= render 'header', article: @article %>



<h1>Article</h1>

<h2> <%= link_to @article.title, article_path(@article) %> </h2>
<p> <%= link_to "Edit article", edit_article_path %> </p>

<p> <%= link_to "Delete", article_path(@article),
      method: :delete,
      data: { confirm: "Are you sure?" } %> </p>

<p>Find me in app/views/articles/show.html.erb</p>
